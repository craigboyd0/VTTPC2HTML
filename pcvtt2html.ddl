CREATE TABLE spell_list
(spell_level INTEGER,
casting_time VARCHAR(25),
components VARCHAR(25),
desc_txt VARCHAR(25),
spell_name VARCHAR(25),
spell_duration VARCHAR(25),
group_txt VARCHAR(25),
prepared_count VARCHAR(25),
range_txt VARCHAR(25),
school_txt VARCHAR(25),
source_txt VARCHAR(25),
action_txt VARCHAR(25));
